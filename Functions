//Function with arguments and with return type

public class functions {
public static void main(String[] str) {
		int x=3,y=34;
		int z=vel(x,y);
		System.out.println(z);
	}
public static int vel(int x,int y) {
	int z=x+y;
	return z;
}
}

//Function without arguments and with return type

public class function2 {
	static int x=3;
	static int y=34;
public static void main(String[] str) {
	int z=vel();
	System.out.println(z);
}
public static int vel() {
int z=x+y;
return z;
}
}

//Function with arguments and without return type

public class function3 {
public static void main(String[] str) {
	 int x=3;
	int y=34;
	vel(x,y);
}
public static void vel(int x,int y) {
    int z=x+y;
    System.out.println(z); 
}
}

//Function without arguments and without return type

public class function4 {
	static int a=12;
	static int b=23;
public static void main(String[] str) {
	vel();
}
public static void vel() {
	int c=a+b;
	System.out.println(c);
}
}
